{ "$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json"
, "name": "Markdown"
, "scopeName": "text.markdown.custom"
, "injectionSelector": "L:text.html.markdown"
, "patterns": 
    [ { "match": "(@{2})(((?!@{2}).)+)(@{2})"
      , "name": "text.markdown.custom.2-at-section"
      , "captures": 
        { "1": { "name": "punctuation.definition.2-at.markdown" }
        , "2": { "name": "markup.other.2-at.markdown" }
        , "4": { "name": "punctuation.definition.2-at.markdown" }
        }
      }
    , { "match": "(#{2})(((?!#{2}).)+)(#{2})"
      , "name": "text.markdown.custom.2-sharp-section"
      , "captures": 
        { "1": { "name": "punctuation.definition.2-sharp.markdown" }
        , "2": { "name": "markup.other.2-sharp.markdown" }
        , "4": { "name": "punctuation.definition.2-sharp.markdown" }
        }
      }
    , { "match": "(%{2})(((?!%{2}).)+)(%{2})"
      , "name": "text.markdown.custom.2-percent-section"
      , "captures": 
        { "1": { "name": "punctuation.definition.2-percent.markdown" }
        , "2": { "name": "markup.other.2-percent.markdown" }
        , "4": { "name": "punctuation.definition.2-percent.markdown" }
        }
      }
    , { "match": "(\\^{2})(((?!\\^{2}).)+)(\\^{2})"
      , "name": "text.markdown.custom.2-cap-section"
      , "captures": 
        { "1": { "name": "punctuation.definition.2-cap.markdown" }
        , "2": { "name": "markup.other.2-cap.markdown" }
        , "4": { "name": "punctuation.definition.2-cap.markdown" }
        }
      }
    , { "match": "(&{2})(((?!&{2}).)+)(&{2})"
      , "name": "text.markdown.custom.2-ampersand-section"
      , "captures": 
        { "1": { "name": "punctuation.definition.2-ampersand.markdown" }
        , "2": { "name": "markup.other.2-ampersand.markdown" }
        , "4": { "name": "punctuation.definition.2-ampersand.markdown" }
        }
      }
    , { "match": "(\\+{2})(((?!\\+{2}).)+)(\\+{2})"
      , "name": "text.markdown.custom.2-plus-section"
      , "captures": 
        { "1": { "name": "punctuation.definition.2-plus.markdown" }
        , "2": { "name": "markup.other.2-plus.markdown" }
        , "4": { "name": "punctuation.definition.2-plus.markdown" }
        }
      }
    , { "match": "(={2})(((?!={2}).)+)(={2})"
      , "name": "text.markdown.custom.2-eq-section"
      , "captures": 
        { "1": { "name": "punctuation.definition.2-eq.markdown" }
        , "2": { "name": "markup.other.2-eq.markdown" }
        , "4": { "name": "punctuation.definition.2-eq.markdown" }
        }
      }
    , { "match": "(\\|{2})(((?!\\|{2}).)+)(\\|{2})"
      , "name": "text.markdown.custom.2-pipe-section"
      , "captures": 
        { "1": { "name": "punctuation.definition.2-pipe.markdown" }
        , "2": { "name": "markup.other.2-pipe.markdown" }
        , "4": { "name": "punctuation.definition.2-pipe.markdown" }
        }
      }
	  ]
}